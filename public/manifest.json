{
  "manifest_version": 3,
  "name": "Chunky Tab",
  "version": "0.1.0",
  "description": "Extension for setting todos on tabs using the chunking method.",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png"
  },
  "action": {
    "default_title": "Chunky Tab",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "https://*/*"
]
}